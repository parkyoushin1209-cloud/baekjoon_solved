import sys
input = sys.stdin.readline
n = int(input())
data=[0]
data += list(map(int,input().split()))
dp = [0] * (n+1)
dp[1] = data[1]
for i in range(2,n+1):
    dp[i] = data[i]
    for j in range(1,i):
        if data[j] < data[i]:
            dp[i] = max(dp[j] + data[i],dp[i])


print(max(dp))
##dp초기값을 data와 동기화시키면 깔끔함
