#define _CRT_SECURE_NO_WARNINGS
#include <stdio.h>
#include "utils.h"
#include <stdlib.h>
#include <string.h>

int n, s;
int* arr;

int cnt = 0;

void dfs(int idx, int total) {
	if (idx == n)
		return;
	total += arr[idx];
	if (total == s)
		cnt++;
	dfs(idx + 1, total);
	dfs(idx + 1, total - arr[idx]);
}

int main() {
	scanf("%d %d", &n, &s);
	arr = (int*)malloc(n * sizeof(int));
	for (int i = 0; i < n; i++)
		scanf(" %d", &arr[i]);
	dfs(0, 0);
	printf("%d\n", cnt);
	free(arr);
}
## 수열에서 인덱스 올려가면서 확인 요소 선택 여부 따져가면서 -> 백트래킹
